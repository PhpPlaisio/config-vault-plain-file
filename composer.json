{
  "name": "plaisio/config-vault-plain-file",
  "description": "PhpPlaisio: Plain File Config Vault",
  "keywords": [
    "PhpPlaisio",
    "Config",
    "Vault"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "ext-json": "*",
    "plaisio/config-vault": "^1.0.0",
    "setbased/exception": "^2.0.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0"
  },
  "autoload": {
    "psr-4": {
