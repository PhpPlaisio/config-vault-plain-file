{
  "name": "plaisio/config-vault-plain-file",
  "description": "PhpPlaisio: Plain File Config Vault",
  "keywords": [
    "PhpPlaisio",
    "Config",
    "Vault"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "plaisio/config-vault": "^1.3.0",
    "setbased/exception": "^2.3.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.6.2"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\ConfigVault\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\ConfigVault\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
